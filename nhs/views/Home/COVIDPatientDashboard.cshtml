<script>
    function GetPage(pPageNo, searchfield, pageSize, SortColumn, frompager, IsTotalTestsOrdered, IsTotalTestsOrderedInPatient, IsPositiveTestCases, IsPositiveTestCasesICU,
        IsNegativeTestCases, IsPositiveDischarges, IsPositiveDeaths, IsPositiveInPatient,
        IsPendingInPatient, IsPositiveLast24hours, IsNegativeLast24hours, IsPositiveICUInPatient,
        IsPendingICUInPatient, IsPositiveDischargeCountLast24hours, IsPositiveDeathCountLast24hours, IsActivePositiveDetected,
        IsActivePositiveDiagnosed, IsFirstTestResultPending, IsFiveSevenReTestPending, IsActiveNegative, IsFiveDaysReTest,
        IsFiveSevenReTestNegative, IsUntested, fromfilter) {
        var pageSize = $("#tblPatientDetails_length").val();
        var dataActionUrl = '@Url.Action("MoreCOVIDPatientDetails", "Home")';
        //alert(IsPositiveDischarges);
        if (frompager == 'onclick')
        {
            //alert(frompager);
            frompager = true;
        }
        if (fromfilter == true) {
            $('#tblPatientDetails_length').val("10").attr("selected", "selected");
            $("#searchfield").val('');
            pageSize = 10;
        }
        $.ajax({
            url: dataActionUrl,
            data: {
                pageNumber: pPageNo, searchfield: searchfield, pageSize: pageSize, SortColumn: SortColumn, IsTotalTestsOrdered: IsTotalTestsOrdered,
                IsTotalTestsOrderedInPatient: IsTotalTestsOrderedInPatient, IsPositiveTestCases: IsPositiveTestCases, IsPositiveTestCasesICU: IsPositiveTestCasesICU,
                IsNegativeTestCases: IsNegativeTestCases,
                IsPositiveDischarges: IsPositiveDischarges, IsPositiveDeaths: IsPositiveDeaths,
                IsPositiveInPatient: IsPositiveInPatient,
                IsPendingInPatient: IsPendingInPatient, IsPositiveLast24hours: IsPositiveLast24hours,
                IsNegativeLast24hours: IsNegativeLast24hours, IsPositiveICUInPatient: IsPositiveICUInPatient,
                IsPendingICUInPatient: IsPendingICUInPatient,
                IsPositiveDischargeCountLast24hours: IsPositiveDischargeCountLast24hours, IsPositiveDeathCountLast24hours: IsPositiveDeathCountLast24hours,
                IsActivePositiveDetected: IsActivePositiveDetected, IsActivePositiveDiagnosed: IsActivePositiveDiagnosed, IsFirstTestResultPending: IsFirstTestResultPending,
                IsFiveSevenReTestPending: IsFiveSevenReTestPending, IsActiveNegative: IsActiveNegative, IsFiveDaysReTest: IsFiveDaysReTest, IsFiveSevenReTestNegative: IsFiveSevenReTestNegative,
                IsUntested: IsUntested, fromfilter: fromfilter, frompager: frompager
            },
            type: "POST",
            success: function (data) {
                $("#dvCustomerList").empty();
                $("#dvCustomerList").html(data);
                //$("#loader-wrapper1").hide();
                //$("#loader").hide();
            },
            error: function (data) {
                //$("#loader-wrapper1").hide();
                //$("#loader").hide();
            }
        });
    }
</script>
<script type="text/javascript">

    $(document).ready(function () {
        //Dropdownlist Selectedchange event
        $("#ddlPatientType").change(function () {
            $("#ddlLastLocation").empty();
            $.ajax({
                type: 'POST',
                url: '@Url.Action("GetLocations")', // we are calling json method

                dataType: 'json',

                data: { patienttype: $("#ddlPatientType").val() },

                success: function (locations) {
                    // states contains the JSON formatted list
                    // of states passed from the controller

                    $.each(locations, function (i, location) {
                        $("#ddlLastLocation").append('<option value="' + location.Value + '">' +
                         location.Text + '</option>');
                    // here we are adding option for States

                    });
                },
                error: function (ex) {
                    alert('Failed to retrieve locations.' + ex);
                }
            });
            return false;
        })
    });
</script>  